/**
 *
 * @author Windows 8.1
 */
public class Rekrusif10 {
    public static boolean golLike(int p, int q){
        if(q == 0)return (p == 1);//jika q sama dengan 0 return p == 1
        return golLike(q,p% q);
    }
    public static void main(String[] args) {
        System.out.println(golLike(1, 0));
    }
}
