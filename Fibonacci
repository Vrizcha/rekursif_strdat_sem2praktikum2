/**
 *
 * @author Windows 8.1
 */
public class Fibonacci {
    public static int Fibbon (int x){
        if(x <= 0 || x <= 1)
            return x;//mereturn x
        else//jika tidak
            return Fibbon (x-2) + Fibbon(x-1);//return Fibbon (x-2) + Fibbon(x-1)
    }
    public static void main(String[] args) {
        int n = 10;//nilai n = 10
        for( int i = 0; i < n; i++)//perulangan atau looping
            System.out.println("F" + i + "=" + Fibbon(i));//menampilkan hasil
    }
}
