/**
 *
 * @author Windows 8.1
 */
public class Kombinasi {
    public static void Kombinasi(String x, int y){
        if(y==0)//jika b sama dengan 0
        System.out.print(x+ " ");//maka tampilkan x+ " "
        else//jika tidak
        for(int i = 97; i < 99; i++){//melooping
                Kombinasi(x+(char) i, y-1);
                }
    }
    
    public static void main(String[] args) {
        Kombinasi("", 2);
    }
     
}
